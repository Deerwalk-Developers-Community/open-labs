---
import OpenLabsLogo from "./open-labs-logo.astro";
import DropDownArrow from "#assets/icons/downArrow.svg";
import Combobox from "#components/Combobox.astro";
const universities = ["KU", "DWIT", "PU", "ALY"];
---

<div
  class="flex flex-col justify-center items-center gap-5 min-h-full w-full px-4"
>
  <div class="">
    <OpenLabsLogo />
  </div>
  <div class="flex flex-col items-center gap-4 w-full max-w-xl">
    <h1 class="font-bold text-2xl text-center">Select Your University</h1>
    <Combobox />
  </div>
</div>
